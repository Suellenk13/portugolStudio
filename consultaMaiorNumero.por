programa
{
	/*
	* Este algoritmo verifica qual (de dois inteiros) é maior.
	* Criado em 02/2021
	*/
	
	funcao inicio()
	{
		real n1,n2
		escreva("Digite um número: ")
		leia(n1)
		escreva("Digite outro número: ")
		leia(n2)
		
		se (n1>n2){
		escreva ("O maior número é:", n1)
		}
		
		
		senao {
		escreva ("O maior número é: ",n2)
		}
	}
}